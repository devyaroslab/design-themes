<template id="themes_page" inherit_id="website.themes">
    <xpath expr="//div[@id='themes-list']" position="before">
      
	  <!-- Themes selection -->
	    
        <div class="col-md-4">
            <div class="well text-center">
                <div class="image">
                    <img src="/theme_clean/static/img/palette/theme_clean_dark.jpg" class="img-responsive" alt="Clean Theme"/>
                </div>
                <div class="options">
                    <h3>Clean Theme Dark</h3>
                    <a class="btn btn-info" href="/website/theme_change?theme_id=theme_clean.theme_clean_dark">Apply</a>
                </div>
            </div>
        </div>
		
		<div class="col-md-4">
            <div class="well text-center">
                <div class="image">
                    <img src="/theme_clean/static/img/palette/theme_clean_light.jpg" class="img-responsive" alt="Clean Theme"/>
                </div>
                <div class="options">
                    <h3>Clean Theme Light</h3>
                    <a class="btn btn-info" href="/website/theme_change?theme_id=theme_clean.theme_clean_light">Apply</a>
                </div>
            </div>
        </div>
		
		<div class="col-md-4">
            <div class="well text-center">
                <div class="image">
                    <img src="/theme_clean/static/img/palette/theme_clean_cold.jpg" class="img-responsive" alt="Clean Theme"/>
                </div>
                <div class="options">
                    <h3>Clean Theme Cold</h3>
                    <a class="btn btn-info" href="/website/theme_change?theme_id=theme_clean.theme_clean_cold">Apply</a>
                </div>
            </div>
        </div>
		
        <div class="col-md-4">
            <div class="well text-center">
                <div class="image">
                    <img src="/theme_clean/static/img/palette/theme_clean_warm.jpg" class="img-responsive" alt="Clean Theme"/>
                </div>
                <div class="options">
                    <h3>Clean Theme Warm</h3>
                    <a class="btn btn-info" href="/website/theme_change?theme_id=theme_clean.theme_clean_warm">Apply</a>
                </div>
            </div>
        </div>
		
    </xpath>
</template>

	<!-- Styles selection -->



<template id="theme_clean.theme_clean_dark" name="Theme Clean" inherit_id="website.theme">
    <xpath expr="//link[@id='bootstrap_css']" position="replace">
        <link rel="stylesheet" type="text/css" href="/theme_clean/static/css/theme_clean_dark.css" t-ignore="true"/>
    </xpath>
</template>

<template id="theme_clean.theme_clean_light" name="Theme Clean" inherit_id="website.theme" optional="disabled">
    <xpath expr="//link[@id='bootstrap_css']" position="replace">
        <link rel="stylesheet" type="text/css" href="/theme_clean/static/css/theme_clean_light.css" t-ignore="true"/>
    </xpath>
</template>

<template id="theme_clean.theme_clean_cold" name="Theme Clean" inherit_id="website.theme">
    <xpath expr="//link[@id='bootstrap_css']" position="replace">
        <link rel="stylesheet" type="text/css" href="/theme_clean/static/css/theme_clean_cold.css" t-ignore="true"/>
    </xpath>
</template>

<template id="theme_clean.theme_clean_warm" name="Theme Clean" inherit_id="website.theme" optional="disabled">
    <xpath expr="//link[@id='bootstrap_css']" position="replace">
        <link rel="stylesheet" type="text/css" href="/theme_clean/static/css/theme_clean_warm.css" t-ignore="true"/>
    </xpath>
</template>

